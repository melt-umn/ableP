# 1 "ForLoopRange_P.pml"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "ForLoopRange_P.pml"
active proctype main()
{
   printf("hello ... a for loop over a range...\\n");
   int i;
   i = 1;
   do
   ::
      (i<=9);
      printf("i=%d\n",i);
      i = (i+1);

   ::
      else;
      goto b0;

   od;
b0: skip;

   printf("End...\n") ;
}


